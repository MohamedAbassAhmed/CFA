@{
    var callingString = "";
    if (TempData["notification"] != null)
    {
        callingString = TempData["notification"].ToString();
        TagBuilder script = new TagBuilder("script");
        script.InnerHtml.Append("$(document).ready(function () {" + callingString + "});");
        //script.ToString();
        var callingScript = "<script>$(document).ready(function () {" + callingString + "});</script>";

        @Html.Raw(callingScript);
    }



}
